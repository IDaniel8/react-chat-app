{
  "name": "chat-app-server",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "private": true,
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean && babel ./src --out-dir ./dist --copy-files",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-requests-logger": "^3.0.2",
    "node-color-log": "^5.2.0",
    "socket.io": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "nodemon": "^2.0.6"
  }
}